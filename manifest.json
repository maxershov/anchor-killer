{
    "manifest_version": 2,
    "name": "Anchor Killer",
    "description": "This extension clear href so you can't accidentally click and go to other page",
    "version": "0.1",
      "browser_action": {
        "default_popup": "popup.html"
       },
      "permissions": [
        "activeTab"
        ],
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ]
  }